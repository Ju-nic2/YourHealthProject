<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yourhealth">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mainmenuActivity"></activity>
        <activity
            android:name=".chooseActivity"
            android:theme="@style/chooseTheme" />
        <activity
            android:name=".chooseimageActivity"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity android:name=".repasswordActivity" />
        <activity android:name=".makeprofileActivity"  android:theme="@style/introTheme" />
        <activity android:name=".memberActivity"  android:theme="@style/introTheme" />
        <activity android:name=".logInActivity"  android:theme="@style/introTheme" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/introTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>